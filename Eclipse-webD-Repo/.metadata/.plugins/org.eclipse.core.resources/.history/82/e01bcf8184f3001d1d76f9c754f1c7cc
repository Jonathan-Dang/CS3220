drop table if exists Users;

create table Users (
	id					integer ,
	name				varchar(255) not null,
	SystoicVal			int not null,
	DiastolicVal		int not null,
	entryTime			datetime
);

insert into Users values (1,"Mom", 110, 90, "2013-03-16 21:01:00");
